
<div class="formboxcenter">
  <div class="formbox">
    <%= simple_form_for(@service) do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :rate %>
      <%= f.input :years_experience %>
      <%= f.input :photos, as: :file, input_html: { multiple: true } %>
      <div class="button-center">
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>

