outgoingCard = document.querySelector("#outgoing-<%= @booking.id %>");
incomingCard = document.querySelector("#incoming-<%= @booking.id %>");

if (outgoingCard) {
  outgoingCard.innerHTML = "<%= j render 'outgoing_card', b: @booking %>";
} else {
  incomingCard.innerHTML = "<%= j render 'incoming_card', b: @booking %>";
}

